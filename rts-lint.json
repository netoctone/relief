{
  "extends": ["tslint:recommended"],
  "rules": {
    "member-access": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "trailing-comma": [false, {"multiline": "always", "singlelint": "never"}]
  }
}
